<% include partials/header %>
<% include partials/navbar %>
<div class= "ui main container">
  <h1>Blogs page</h1>
  <% blogs.forEach(function(blog){ %>
    <div>
      <h2><%= blog.title %></h2>
      <img src="<%= blog.image %>">
      <span> <%= blog.created %></span>
      <p><%= blog.body %></p>
      <a href="/blogs/<%= blog._id %>">Read More</a>
    </div>
  <% }) %>
</div>
<% include partials/footer %>
